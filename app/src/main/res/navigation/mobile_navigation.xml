<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.kushwaha.omnipractice.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_Summary"
        android:name="com.kushwaha.omnipractice.ui.dashboard.DashboardFragment"
        android:label="@string/title_Summary"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_Profile"
        android:name="com.kushwaha.omnipractice.ui.notifications.NotificationsFragment"
        android:label="@string/title_Profile"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.kushwaha.omnipractice.ui.setting.SettingsFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_settings" />

</navigation>